{
    "name": "saeh-website",
    "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "setup": "node scripts/setup.js",
    "check": "node scripts/check-setup.js",
    "seed": "node scripts/seed.js",
    "db:push:postgres": "npx prisma db push --schema=./prisma/schema.postgres.prisma",
    "db:push:mongo": "npx prisma db push --schema=./prisma/schema.mongo.prisma",
    "db:generate": "npx prisma generate --schema=./prisma/schema.postgres.prisma && npx prisma generate --schema=./prisma/schema.mongo.prisma"
  },
    "dependencies": {
      "next": "14.0.0",
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "next-auth": "4.24.5",
      "prisma": "5.6.0",
      "@prisma/client": "5.6.0",
      "bcryptjs": "2.4.3",
      "quill": "1.3.7",
      "react-quill": "2.0.0",
      "@mui/icons-material": "5.15.0",
      "multer": "1.4.5-lts.1"
    },
    "devDependencies": {
      "autoprefixer": "10.4.16",
      "postcss": "8.4.31",
      "tailwindcss": "3.3.5"
    }
  }